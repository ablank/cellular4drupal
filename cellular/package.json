  "scripts": {
    "clean:assets": "rimraf dist/assets",
    "clean:css": "rimraf dist/css/*",
    "clean:js": "rimraf dist/js",
    "clean:dist": "rimraf dist/{css,js,assets}/**/*",
    "postcss": "node src/_build/postcss.js --trace-warnings ...",
    "mincss": "postcss dist/css/ -u cssnano -d dist/css/ --ext .min.css",
    "sass": "sass src/sass:dist/css --no-source-map",
    "js:beautify": "copyfiles -f src/js/*.js dist/js && prettier --config ./src/_build/.prettierrc.json --write ./dist/js/**/*",
    "js:uglify": "uglifyjs-folder dist/js -eo dist/js",
    "imagemin": "imagemin dist/assets/**/*",
    "favicons": "imagemin src/assets/favicons/ -o dist/assets/favicons",
    "icons": "svgo -f src/assets/icons/svg -o dist/assets/icons/svg && svg-sprite-generate -d src/assets/icons/svg -o src/assets/icons/svg/_sprite.svg && node src/_build/icons.js",
    "build:css": "run-s clean:css sass postcss",
    "build:js": "run-s clean:js js:*",
    "build:images": "run-s clean:assets && node src/_build/images.mjs && run-p favicons icons",
    "build": "run-p build:*",
    "lint:js": "eslint -c src/_build/.eslintrc.json src/js",
    "lint:css": "stylelint dist/css/ --config .stylelintrc.json --fix",
    "lint:sass": "stylelint src/sass/ --config .sasslintrc.json --fix",
    "lint": "run-s lint:*",
    "serve": "browser-sync start --server --files \"dist/css/*.css, dist/js/*.js, **/*.html, !node_modules/**/*\"",
    "watch:sass": "onchange \"src/sass\" -- run-s build:css lint:css",
    "watch:js": "onchange \"src/js\" -- run-s build:js",
    "watch:images": "onchange \"src/assets/images\" -- run-s build:images || onchange \"src/assets/icons\" -- run-s build:icons",
    "watch": "run-p watch:*",
    "postinstall": "run-p build watch"
